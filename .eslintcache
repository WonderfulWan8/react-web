oading = true;\r\n        },\r\n        [searchProduct.fulfilled.type]: (state, action) => {\r\n            // 使用action的payload更新状态state\r\n            state.data = action.payload.data; //action.payload带来的就是api的返回数据\r\n            state.pagination = action.payload.pagination;\r\n            state.loading = false;\r\n            state.error = null;\r\n        },\r\n        [searchProduct.rejected.type]: (\r\n            state,\r\n            action: PayloadAction<string | null>\r\n        ) => {\r\n            const add = action.payload;\r\n            state.loading = false;\r\n            state.error = action.payload;\r\n        },\r\n    },\r\n});\r\n","D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\redux\\user\\slice.ts",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\redux\\shoppingCart\\slice.ts",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\redux\\language\\languageActions.ts",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\redux\\recommendProducts\\recommendProductsAction.ts",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\pages\\index.tsx",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\pages\\shoppingCart\\index.ts",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\pages\\search\\index.ts",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\pages\\placeOrder\\index.ts",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\pages\\home\\index.tsx",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\pages\\signIn\\index.tsx",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\pages\\register\\index.tsx",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\pages\\detail\\index.tsx",[],"D:\\react_version\\reactTS\\05【主页开发】走马灯与侧边栏多重菜单\\react-travel\\src\\pages\\shoppingCart\\ShoppingCartPage.tsx",[],"D:\\re